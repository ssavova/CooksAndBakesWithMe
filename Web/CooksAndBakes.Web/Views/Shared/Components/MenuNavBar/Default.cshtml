@model CooksAndBakes.Web.ViewModels.Categories.AllCategoriesViewModel

<hr />
<nav class="navbar navbar-expand-lg navbar-dark mb-2">
    <div class="container ">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
            <ul class="navbar-nav flex-grow-1  mr-auto text-center">
                @foreach (var name in Model.Categories)
                {
                    <li class="nav-item ">
                        <a class="nav-link text-dark text-uppercase font-weight-bold" href="">@name.Title</a>
                    </li>
                }
            </ul>
        </div>
    </div>
</nav>
<hr />


